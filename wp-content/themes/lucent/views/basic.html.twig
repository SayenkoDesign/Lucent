{% extends 'base.html.twig' %}
{% block header %}
    {% include "layout/menu.html.twig" %}
    {{ parent() }}
{% endblock %}
{% block content %}
    {% if rows %}
        {% for row in rows %}
            {{ row }}
        {% endfor %}
    {% else %}
        <section class="content">
            <div class="row column">
                {{ SP.WordPress.content() }}
            </div>
        </section>
    {% endif %}
{% endblock %}